# Test Environment Configuration
# Copy this to .env.test.local and fill in real values

# Test Database (separate from production)
DATABASE_URL="postgresql://citizenship_user:citizenship_pass@localhost:5433/citizenship_test_db"

# OpenAI API for embeddings in tests (not Realtime API)
OPENAI_API_KEY="sk-test-your-openai-api-key-here"

# Session configuration for tests
SESSION_SECRET="test-session-secret-not-for-production"

# OAuth providers (not needed for most integration tests)
GOOGLE_CLIENT_ID="test-google-client-id"
GOOGLE_CLIENT_SECRET="test-google-client-secret"
FACEBOOK_APP_ID="test-facebook-app-id"
FACEBOOK_APP_SECRET="test-facebook-app-secret"
MICROSOFT_CLIENT_ID="test-microsoft-client-id"
MICROSOFT_CLIENT_SECRET="test-microsoft-client-secret"

# Test-specific settings
NODE_ENV="test"
PORT="3001"